<a class="wbmi-input-widget__autocomplete" href={{url}}>
	<span class="wbmi-input-widget__autocomplete__label">
		{{label}}
	</span>

	{{#aliases}}
	<span class="wbmi-input-widget__autocomplete__aliases">
		{{aliases}}
	</span>
	{{/aliases}}

	{{#description}}
	<span class="wbmi-input-widget__autocomplete__description">
		{{description}}
	</span>
	{{/description}}
</a>
