<div class="wbmi-qualifier">
	{{#editing}}
		{{{propertyInput}}}
		{{{valueInput}}}
		{{{removeIcon}}}
	{{/editing}}

	{{^editing}}
		<div class="wbmi-qualifier-value">
			{{{property}}}

			{{#property}}
				{{#value}}
				<span class="wbmi-qualifier-value-separator">{{separator}}&#8232;</span>
				{{/value}}
			{{/property}}

			{{{value}}}
		</div>
	{{/editing}}
</div>
