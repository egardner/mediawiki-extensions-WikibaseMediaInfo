{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery"
	],
	"env": {
		"commonjs": true
	},
	"globals": {
		"module": false,
		"util": false,
		"wikibase": false,
		"dataValues": false,
		"mw": false,
		"OO": false
	},
	"rules": {
		"no-underscore-dangle": "off",
		"one-var": "off",
		"vars-on-top": "off",
		"valid-jsdoc": "off"
	}
}
