{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery"
	],
	"env": {
		"commonjs": true
	},
	"globals": {
		"module": false,
		"util": false,
		"wikibase": false,
		"dataValues": false,
		"globeCoordinate": false,
		"mw": false,
		"OO": false
	},
	"rules": {
		"max-len": [
			"warn", {
				"code": 110,
				"ignoreComments": true
			}
		]
	}
}
