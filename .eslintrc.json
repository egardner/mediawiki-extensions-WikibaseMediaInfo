{
	"extends": "wikimedia",
	"env": {
		"browser": true,
		"jquery": true
	},
	"globals": {
		"module": false,
		"util": false,
		"wikibase": false,
		"mediaWiki": false,
		"OO": false
	},
	"rules": {
		"dot-notation": [ "error", { "allowKeywords": true } ],
		"no-underscore-dangle": "off",
		"one-var": "off",
		"vars-on-top": "off",
		"max-len": 0,
		"valid-jsdoc": 0
	}
}
