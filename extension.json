{
	"name": "WikibaseMediaInfo",
	"version": "0.1.0",
	"author": [
		"The Wikidata team"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:WikibaseMediaInfo",
	"descriptionmsg": "wikibasemediainfo-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "wikibase",
	"requires": {
		"MediaWiki": ">= 1.32.0",
		"extensions": {
			"UniversalLanguageSelector": "*"
		}
	},
	"AutoloadNamespaces": {
		"Wikibase\\MediaInfo\\": "src/",
		"Wikibase\\MediaInfo\\Tests\\": "tests/phpunit/composer/",
		"Wikibase\\MediaInfo\\Tests\\MediaWiki\\": "tests/phpunit/mediawiki/"
	},
	"config": {
		"MediaInfoEnable": {
			"value": true
		},
		"MediaInfoEnableFilePageDepicts": {
			"value": false
		},
		"MediaInfoProperties": {
			"description": "Map of { indicative name: property id }. E.g. { depicts: P1 }",
			"value": {
				"depicts": "P1"
			}
		},
		"DepictsQualifierProperties": {
			"description": "Map of all available properties for depicts qualifiers dropdown. Map of { (unused) indicative name: property id }. E.g. { color: P2, wears: P3 }",
			"value": {},
			"merge_strategy": "array_plus_2d"
		},
		"MediaInfoShowQualifiers": {
			"value": true
		},
		"MediaInfoExternalEntitySearchBaseUri": {
			"value": ""
		}
	},
	"ServiceWiringFiles": [
		"src/Services/MediaInfoServiceWiring.php"
	],
	"callback": "Wikibase\\MediaInfo\\WikibaseMediaInfoHooks::onRegistration",
	"Hooks": {
		"MediaWikiServices": [
			"Wikibase\\MediaInfo\\WikibaseMediaInfoHooks::onMediaWikiServices"
		],
		"WikibaseRepoEntityNamespaces": [
			"Wikibase\\MediaInfo\\WikibaseMediaInfoHooks::onWikibaseRepoEntityNamespaces"
		],
		"WikibaseRepoEntityTypes": [
			"Wikibase\\MediaInfo\\WikibaseMediaInfoHooks::onWikibaseEntityTypes"
		],
		"WikibaseClientEntityTypes": [
			"Wikibase\\MediaInfo\\WikibaseMediaInfoHooks::onWikibaseEntityTypes"
		],
		"GetEntityByLinkedTitleLookup": [
			"Wikibase\\MediaInfo\\WikibaseMediaInfoHooks::onGetEntityByLinkedTitleLookup"
		],
		"BeforePageDisplay": [
			"Wikibase\\MediaInfo\\WikibaseMediaInfoHooks::onBeforePageDisplay"
		],
		"CirrusSearchBuildDocumentParse": [
			"Wikibase\\MediaInfo\\WikibaseMediaInfoHooks::onCirrusSearchBuildDocumentParse"
		],
		"ParserOutputPostCacheTransform": [
			"Wikibase\\MediaInfo\\WikibaseMediaInfoHooks::onParserOutputPostCacheTransform"
		]
	},
	"MessagesDirs": {
		"WikibaseMediaInfo": [
			"i18n"
		]
	},
	"ResourceModules": {
		"wikibase.mediainfo.getDeserializer": {
			"scripts": "resources/getDeserializer.js",
			"dependencies": [
				"wikibase.mediainfo.serialization.MediaInfoDeserializer"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"wikibase.mediainfo.serialization.MediaInfoDeserializer": {
			"scripts": [
				"resources/__namespace.js",
				"resources/datamodel/__namespace.js",
				"resources/datamodel/MediaInfo.js",
				"resources/serialization/__namespace.js",
				"resources/serialization/MediaInfoDeserializer.js"
			],
			"dependencies": [
				"util.inherit",
				"wikibase.datamodel.Entity",
				"wikibase.datamodel.Fingerprint",
				"wikibase.datamodel.StatementGroupSet",
				"wikibase.datamodel.TermMap",
				"wikibase.serialization.Deserializer",
				"wikibase.serialization.StatementGroupSetDeserializer",
				"wikibase.serialization.TermMapDeserializer"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"wikibase.mediainfo.filePageDisplay": {
			"scripts": [
				"resources/filepage/__namespace.js",
				"resources/filepage/LanguagesViewWidget.js",
				"resources/filepage/LicenseDialogWidget.js",
				"resources/filepage/UlsWidget.js",
				"resources/filepage/CancelPublishWidget.js",
				"resources/filepage/CaptionsEditActionsWidget.js",
				"resources/filepage/CaptionData.js",
				"resources/filepage/CaptionsPanel.js",
				"resources/filepage/DepictsPanel.js",
				"resources/filepage/init.js"
			],
			"dependencies": [
				"oojs",
				"oojs-ui-core",
				"oojs-ui-widgets",
				"oojs-ui-windows",
				"oojs-ui.styles.icons-editing-core",
				"oojs-ui.styles.icons-movement",
				"wikibase.mediainfo.filepage.styles",
				"wikibase.mediainfo.statements",
				"wikibase",
				"wikibase.api.getLocationAgnosticMwApi",
				"wikibase.api.RepoApi",
				"wikibase.api.RepoApiError",
				"ext.uls.mediawiki"
			],
			"messages": [
				"wikibasemediainfo-filepage-structured-data-heading",
				"wikibasemediainfo-filepage-captions-title",
				"wikibasemediainfo-filepage-more-languages",
				"wikibasemediainfo-filepage-fewer-languages",
				"wikibasemediainfo-filepage-add-caption",
				"wikibasemediainfo-filepage-edit-captions",
				"wikibasemediainfo-filepage-edit-depicts",
				"wikibasemediainfo-filepage-cancel",
				"wikibasemediainfo-filepage-edit",
				"wikibasemediainfo-filepage-publish",
				"wikibasemediainfo-filepage-caption-empty",
				"wikibasemediainfo-filepage-caption-too-long",
				"wikibasemediainfo-filepage-caption-approaching-limit",
				"wikibasemediainfo-filepage-license-title",
				"wikibasemediainfo-filepage-license-content",
				"wikibasemediainfo-filepage-license-content-acceptance",
				"wikibasemediainfo-filepage-license-accept",
				"wikibasemediainfo-filepage-license-cancel"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"wikibase.mediainfo.filepagestyles": {
			"styles": [
				"resources/mediainfo-filepage.less"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"wikibase.mediainfo.filepage.styles": {
			"styles": [
				"resources/mediainfo-filepage.less"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"wikibase.mediainfo.statements": {
			"scripts": [
				"resources/statements/__namespace.js",
				"resources/statements/FormatValueElement.js",
				"resources/statements/EntityLookupElement.js",
				"resources/statements/EntityInputWidget.js",
				"resources/statements/QualifierValueInputWidget.js",
				"resources/statements/DepictsWidget.js",
				"resources/statements/ItemInputWidget.js",
				"resources/statements/ItemWidget.js",
				"resources/statements/QualifierWidget.js"
			],
			"dependencies": [
				"oojs",
				"oojs-ui-core",
				"oojs-ui-widgets",
				"wikibase.mediainfo.statements.styles",
				"wikibase.serialization.StatementListSerializer",
				"wikibase.serialization.StatementListDeserializer",
				"wikibase.serialization.StatementSerializer",
				"wikibase.serialization.StatementDeserializer",
				"wikibase.datamodel.Statement",
				"wikibase.datamodel.EntityId",
				"wikibase.utilities.ClaimGuidGenerator",
				"dataValues.values"
			],
			"messages": [
				"wikibasemediainfo-property-placeholder",
				"wikibasemediainfo-statements-depicts-title",
				"wikibasemediainfo-statements-item-input-placeholder",
				"wikibasemediainfo-statements-item-input-label",
				"wikibasemediainfo-statements-item-is-primary",
				"wikibasemediainfo-statements-item-make-primary",
				"wikibasemediainfo-statements-item-remove",
				"wikibasemediainfo-statements-item-add-qualifier",
				"wikibasemediainfo-filepage-statement-no-results",
				"colon-separator",
				"parentheses"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"wikibase.mediainfo.statements.styles": {
			"styles": [
				"resources/statements/EntityLookupElement.less",
				"resources/statements/EntityInputWidget.less",
				"resources/statements/QualifierValueInputWidget.less",
				"resources/statements/DepictsWidget.less",
				"resources/statements/ItemInputWidget.less",
				"resources/statements/ItemWidget.less",
				"resources/statements/QualifierWidget.less"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "WikibaseMediaInfo"
	},
	"SpecialPages": {},
	"manifest_version": 2,
	"GroupPermissions": {
		"*": {
			"mediainfo-term": true
		}
	},
	"AvailableRights": [
		"mediainfo-term"
	]
}
